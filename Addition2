//java program to add two integers
/*file name:Addition2.java
date:16/08/2024
author:hemanth*/
import java.io.*;
class Addition2{
    public static void main(String[] args){
        int x,y,z;
        x=Integer.parseInt(args[0]);
        y=Integer.parseInt(args[1]);
        z=x+y;
        System.out.println("Sum of "+x+" and "+y+" is "+z);
    }
}
